  <ins:set name="Notes">

    <!-- Chalumeau Register: E3 to B4 -->
    <ins:note name="E3 / Fb3">
      <ins:fingering name="Left" comment="Use in combination with fingerings using right little finger or no little fingers" keysDown="T,L-1,L-2,L-3,L-E,R-1,R-2,R-3"/>
      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="T,L-1,L-2,L-3,R-1,R-2,R-3,R-E"/>
      <ins:fingering name="F3/C5" comment="Use in combination with F3 or C5" keysDown="T,L-1,L-2,L-3,L-E,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="E#3 / F3">
