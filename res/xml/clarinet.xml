      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="T,L-1,L-2,L-3,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="F#3 / Gb3">
      <ins:fingering name="Left" comment="Use in combination with fingerings using right little finger or no little fingers" keysDown="T,L-1,L-2,L-3,L-F#,R-1,R-2,R-3"/>
      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="T,L-1,L-2,L-3,R-1,R-2,R-3,R-F#"/>
      <ins:fingering name="F3/C5" comment="Use in combination with F3 or C5" keysDown="T,L-1,L-2,L-3,L-F#,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="G3">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,R-1,R-2,R-3"/>
    </ins:note>
    <ins:note name="G#3 / Ab3">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,R-1,R-2,R-3,R-G#"/>
      <ins:fingering name="LH-G" comment="For models with LH G#-key, use in combination with fingerings using right little finger or no little fingers." keysDown="T,L-1,L-2,L-3,L2-G#,R-1,R-2,R-3"/>
    </ins:note>
    <ins:note name="A3">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,R-1,R-2"/>
    </ins:note>
    <ins:note name="A#3 / Bb3">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,R-1"/>
    </ins:note>
    <ins:note name="B3 / Cb4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,R-2"/>
      <ins:fingering name="Basic" comment="Chromatic, use in combination with Bb3 or F4" keysDown="T,L-1,L-2,L-3,R-1,R-B"/>
    </ins:note>
    <ins:note name="B#3 / C4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3"/>
    </ins:note>
    <ins:note name="C#4 / Db4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,L-3,L-C#"/>
    </ins:note>
    <ins:note name="D4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2"/>
    </ins:note>
    <ins:note name="D#4 / Eb4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1,L-2,RT-4"/>
      <ins:fingering name="Chromatic" comment="Chromatic and trill fingering, use in combination with D4 and A5" keysDown="T,L-1,L-2,L-Eb"/>
      <ins:fingering name="RH1" comment="Sharp, use for facility in combination with fingerings involving RH 1" keysDown="T,L-1,R-1"/>
      <ins:fingering name="RH2" comment="Sharp, use for facility in combination with fingerings involving RH 2" keysDown="T,L-1,R-2"/>
      <ins:fingering name="7-Ring" comment="For 7-ring models, use in combination with C4" keysDown="T,L-1,L-3"/>
    </ins:note>
    <ins:note name="E4 / Fb4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T,L-1"/>
      <ins:fingering name="Rounnd" comment="Rounder and lower in pitch than basic fingering" keysDown="T,L-1,L-Eb,RT-4,R-3,R-F"/>
      <ins:fingering name="Long" comment="Produces louder, clear sound, especially useful for long notes" keysDown="T,L-1,L-Eb,L-C#,RT-4,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="E#4 / F4">
      <ins:fingering name="Basic" comment="Basic" keysDown="T"/>
    </ins:note>
    <ins:note name="F#4 / Gb4">
      <ins:fingering name="Basic" comment="Try using RH-1,2,3 in different conbinations for better intonation and tone" keysDown="L-1"/>
      <ins:fingering name="Chromatic" comment="Chromatic, use in combination with F4. Try using RH-1,2,3 in different conbinations for better intonation and tone" keysDown="T,RT-3,RT-4"/>
    </ins:note>
    <ins:note name="G4">
      <ins:fingering name="Basic" comment="Basic. Try using RH-1,2,3 in different conbinations for better intonation and tone" keysDown=""/>
    </ins:note>
    <ins:note name="G#4 / Ab4">
      <ins:fingering name="Basic" comment="Basic. Try different RH combinations for better intonation and tone" keysDown="L-G#"/>
      <ins:fingering name="F4" comment="Use in combination with F4. Try different RH combinations for better intonation and tone" keysDown="T,L-G#,RT-3,RT-4"/>
      <ins:fingering name="Flat" comment="Flatter" keysDown="L-G#,L-2,L-3,RT-4,R-1"/>
      <ins:fingering name="Sharp" comment="Use in combination with E4. G4 is sharp" keysDown="T,L-1,RT-2"/>
    </ins:note>
    <ins:note name="A4">
      <ins:fingering name="Basic" comment="Basic. Try using RH-1,2,3 in different conbinations for better intonation and tone" keysDown="L-A"/>
      <ins:fingering name="Full" comment="Fuller sound without sounding airy" keysDown="L-A,R-2,R-3,R-F"/>
      <ins:fingering name="Flat" comment="Flatter" keysDown="L-A,L-2,L-3,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="A#4 / Bb4">
      <ins:fingering name="Basic" comment="Basic. Try different RH combinations for better intonation and tone." keysDown="R,L-A"/>
      <ins:fingering name="Trill-A4" comment="Trill fingering with A4. Try different RH combinations for better intonation and tone." keysDown="L-A,RT-2"/>
      <ins:fingering name="Alt1" comment="Better in tune and clearer than basic fingering" keysDown="R,L-A,L-3,R-1,R-3,R-F"/>
      <ins:fingering name="Alt2" comment="Better in tune and clearer than basic fingering" keysDown="R,L-A,L-3,L-F,R-1,R-3"/>
      <ins:fingering name="Alt3" comment="Good intonation and smooth transitions with other notes" keysDown="R,L-A,L-1,L-2,L-3,R-2,R-3"/>
      <ins:fingering name="Alt4" comment="Fuller sound; in tune; less awkward" keysDown="R,L-A,L-2,L-3,R-2,R-3,R-F"/>
      <ins:fingering name="Alt5" comment="In tune and fuller, less airy sound" keysDown="R,L-A,L-3,R-3,R-F"/>
    </ins:note>

    <!-- Clarion Register -->
    <ins:note name="-- Clarion Register --">
        <ins:fingering name="B4-C6" comment="" />
    </ins:note>
    
    <ins:note name="B4 / Cb5">
      <ins:fingering name="Left" comment="Use in combination with fingerings using right little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,L-E,R-1,R-2,R-3"/>
      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3,R-E"/>
      <ins:fingering name="C5" comment="Use in combination with C5" keysDown="R,T,L-1,L-2,L-3,L-E,R-1,R-2,R-3,R-F"/>
      <ins:fingering name="TrillA4" comment="Trill fingering with A4" keysDown="L-A,RT-1"/>
      <ins:fingering name="TrillBb4" comment="Trill fingering with Bb4" keysDown="R,L-A,RT-2"/>
      <ins:fingering name="Alt1" comment="Suitable for sustained notes.  Gives a more open sound than basic fingering" keysDown="R,T,L-A,L-1,L-2,L-3,L-E,R-1,R-2,R-3"/>
      <ins:fingering name="Alt2" comment="Suitable for sustained notes.  Gives a more open sound than basic fingering" keysDown="R,T,L-A,L-1,L-2,L-3,R-1,R-2,R-3,R-E"/>
      <ins:fingering name="Alt3" comment="Suitable for sustained notes.  Gives a more open sound than basic fingering" keysDown="R,T,L-A,L-1,L-2,L-3,L-E,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="B#4 / C5">
      <ins:fingering name="Left" comment="Use in combination with fingerings using right little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,L-F,R-1,R-2,R-3"/>
      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3,R-F"/>
      <ins:fingering name="TrillBb4" comment="Trill fingering with Bb4" keysDown="R,L-A,RT-1"/>
      <ins:fingering name="TuneBb4" comment="Trill fingering with Bb4 which is more in tune" keysDown="R,L-A,RT-1,RT-2"/>
    </ins:note>
    <ins:note name="C#5 / Db5">
      <ins:fingering name="Left" comment="Use in combination with fingerings using right little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,L-F#,R-1,R-2,R-3"/>
      <ins:fingering name="Right" comment="Use in combination with fingerings using left little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3,R-F#"/>
      <ins:fingering name="C5" comment="Use in combination with C5" keysDown="R,T,L-1,L-2,L-3,L-F#,R-1,R-2,R-3,R-F"/>
    </ins:note>
    <ins:note name="D5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3"/>
    </ins:note>    
    <ins:note name="D#5 / Eb5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3,R-G#"/>
      <ins:fingering name="LH-G#" comment="For models with LH G# key, use in combination with fingerings using right little finger or no little fingers" keysDown="R,T,L-1,L-2,L-3,L2-G#,R-1,R-2,R-3,R"/>
    </ins:note>    
    <ins:note name="E5 / Fb5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,R-1,R-2"/>
    </ins:note>    
    <ins:note name="E#5 / F5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,R-1"/>
    </ins:note>    
    <ins:note name="F#5 / Gb5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,R-2"/>
      <ins:fingering name="Chromatic" comment="Chromatic, use in combination with Bb4 or F5" keysDown="R,T,L-1,L-2,L-3,R-B"/>
      <ins:fingering name="WithD5" comment="Use in combination with D5" keysDown="R,T,L-1,L-2,L-3,R-3"/>
    </ins:note>    
    <ins:note name="G5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3"/>
    </ins:note>    
    <ins:note name="G#5 / Ab5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,L-3,L-C#"/>
      <ins:fingering name="RH" comment="Use in combination with fingerings involving the right hand fingers" keysDown="R,T,L-1,L-2,R-1,R-2"/>
    </ins:note>    
    <ins:note name="A5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2"/>
    </ins:note>    
    <ins:note name="A#5 / Bb5">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1,L-2,RT-4"/>
      <ins:fingering name="Chromatic" comment="Chromatic and trill fingering, use in combination with D4 and A5" keysDown="R,T,L-1,L-2,L-Eb"/>
      <ins:fingering name="Trill" comment="Trill fingering with Ab5" keysDown="R,T,L-1,L-2,L-C#"/>
      <ins:fingering name="RH-1" comment="Use in combination with fingerings involving RH 1" keysDown="R,T,L-1,R-1"/>
      <ins:fingering name="Sharp" comment="Sharp, use for facility in combination with fingerings involving RH 2" keysDown="R,T,L-1,R-2"/>
      <ins:fingering name="7-ring" comment="For 7-ring models. Use in combination with G5" keysDown="R,T,L-1,L-3"/>
      <ins:fingering name="ppp" comment="For ppp entrances" keysDown="R,T,L-1,L-3,R-1,R-2,R-3,R-F"/>
    </ins:note>    
    <ins:note name="B5 / Cb6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-1"/>
      <ins:fingering name="Alt1" comment="Use in combination with lower altissimo notes" keysDown="R,T,L-2,L-3,L-F#,R-1,R-2,R-3"/>
    </ins:note>    
    <ins:note name="B#5 / C6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T"/>
      <ins:fingering name="pp-D5" comment="Useful for pp attacks and upward skips from D5" keysDown="R,T,L-3,RT-4,R-1,R-2,R-3"/>
      <ins:fingering name="pp-interval" comment="Suitable for large interval skips, such as with D5, and for pp" keysDown="R,T,R-1,R-2,R-3"/>
      <ins:fingering name="sharp-ff" comment="Slightly sharp. Good for ff. Useful in combination with Bb5" keysDown="R,T,L-1,L-2,L-Eb,RT-3,RT-4"/>
      <ins:fingering name="LegatoD6" comment="For legato slurring from D6" keysDown="R,T,L-1,L-2,L-3,R-1,R-2,R-3,R-G#"/>
      <ins:fingering name="ff1" comment="Good intonation and good for ff" keysDown="R,T,L-2,L-3,R-1,R-2,R-3"/>
      <ins:fingering name="ff2" comment="Good intonation and good for ff" keysDown="R,T,L-2,L-3,R-1,R-2,R-3,R-G#"/>
      <ins:fingering name="Bass" comment="For bass clarinet. Use in combination with C6, D6, and E6" keysDown="R,T,L-0,L-3,R-1,R-2,R-3,R-G#"/>
    </ins:note>    
    
    <!-- Lower Altissimo Register -->
    <ins:note name="-- Lower Altissimo Register --">
        <ins:fingering name="C#6 - G6" comment="" />
    </ins:note>
    
    <ins:note name="B5 / Cb6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,L-3,R-1,R-2"/>
      <ins:fingering name="TrillC6" comment="" keysDown="R,T,RT-3,RT-4"/>
      <ins:fingering name="TrillB5" comment="" keysDown="R,T,L-1,RT-4"/>
      <ins:fingering name="HarmonicF#" comment="Harmonic F#4" keysDown="L-1"/>
      <ins:fingering name="HarmonicE" comment="Harmonic E5" keysDown="R,T,L-G#,L-1,L-2,L-3,R-1,R-2,R-G#"/>
      <ins:fingering name="Full" comment="Full sound and great response at pp" keysDown="R,L-1,L-2,L-3,R-1,R-2,R-G#"/>
    </ins:note>    
    <ins:note name="D6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,L-3,R-1,R-G#"/>
      <ins:fingering name="TrillC1" comment="Trill fingering with C6" keysDown="R,T,RT-2"/>
      <ins:fingering name="TrillC2" comment="Less flat, trill fingering with C6" keysDown="R,T,L-G#,RT-4"/>
      <ins:fingering name="TrillC3" comment="Trill fingering with C6" keysDown="R,T,L-G#"/>
      <ins:fingering name="HarmonicG" comment="Harmonic (G4)" keysDown="R,T,R-G#"/>
      <ins:fingering name="Ful" comment="Full tone, good intonation, and great response at pp" keysDown="R,T,L-G#,L-2,L-3,R-1,R-2"/>
      <ins:fingering name="Secure" comment="Secure at pp, useful for wide interval leaps" keysDown="R,L-1,L-2,L-3,R-1,R-3,R-G#"/>
      <ins:fingering name="mf-ff" comment="Good intonation and response. Works well at mf to ff dynamics" keysDown="R,T,L-2,L-3,R-2,R-3,R-G#"/>
    </ins:note>    
    <ins:note name="D#6 / Eb6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,L-3,R-1,R-B,R-G#"/>
      <ins:fingering name="Lower" comment="Use in combination with lower clarion notes" keysDown="R,T,L-2,L-3,R-3,R-G#"/>
      <ins:fingering name="Flat" comment="Flat, use for facility, trill fingering with C#6" keysDown="R,T,L-2,L-3,R-2,R-G#"/>
      <ins:fingering name="HarmonicA" comment="Harmonic (flat A4)" keysDown="R,T,L-A,R-G#"/>
      <ins:fingering name="Dark" comment="Dark, covered sound. Great for pp passages" keysDown="R,L-1,L-2,L-3,R-2,R-G#"/>
    </ins:note>    
    <ins:note name="E6 / Fb6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,L-3,R-G#"/>
      <ins:fingering name="Alt1" comment="Basic" keysDown="R,T,L-G#,L-2,L-3"/>
      <ins:fingering name="Alt2" comment="Basic" keysDown="R,L-1,L-2,L-3,R-G#"/>
    </ins:note>    
    <ins:note name="E#6 / F6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,L-3,L-C#,R-G#"/>
      <ins:fingering name="pp" comment="Use for slurring with lower registers and for pp" keysDown="R,T,L-1,L-2,L-3,L-C#,R-1,R-2,R-3"/>
      <ins:fingering name="LH-Only" comment="Useful for wide interval skips with any LH-only fingering. Quick response" keysDown="R,T,L-1,L-2,L-3,L-C#,RT-2,R-G#"/>
      <ins:fingering name="ff" comment="Easy to finger (LH only). Free blowing and good for ff" keysDown="R,T,L-A,L-1,L-2,L-3,L-C#"/>
      <ins:fingering name="Alt" comment="" keysDown="R,T,L-G#,L-2,L-3,L-C#,R-G#"/>
    </ins:note>    
    <ins:note name="F#6 / Gb6">
      <ins:fingering name="Basic" comment="Basic" keysDown="R,T,L-2,R-G#"/>
      <ins:fingering name="Slurs" comment="Use for slurs with lower registers, more stable, good for pp and ff" keysDown="R,T,L-1,L-2,R-1,R-2,R-3,R-G#"/>
      <ins:fingering name="Speaks" comment="Accurate pitch. Speaks easily in all dynamics. Good for wide skips" keysDown="R,T,L-1,L-2,RT-4,R-G#"/>
      <ins:fingering name="Alt1" comment="" keysDown="R,T,L-2,R-2,R-F#"/>
